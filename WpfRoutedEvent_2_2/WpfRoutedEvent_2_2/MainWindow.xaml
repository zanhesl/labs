        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfRoutedEvent_2_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtBox" HorizontalAlignment="Left" Margin="202,110,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <StackPanel Margin="80,167,0,0" ButtonBase.Click="StackPanel_Click">
            <Button x:Name="butAdd" Content="Button"/>
            <Button x:Name="butMult" Content="Button"/>
        </StackPanel>

    </Grid>
</Window>
